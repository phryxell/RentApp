@model RentApp.Models.TypeOfEquipment

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Radera kategori</h3>
<br />
<p>Om kategorin har utgått från sortimentet kan du radera denna. Är du säker på att du vill radera kategori?</p>
<br />
<div>
    <h5>Du kan radera nedan följande kategori:</h5>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Kategori
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="EquipmentId" />
        <input type="submit" value="Radera" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-outline-dark">Tillbaka <i class="fas fa-backward"></i></a>
    </form>
</div>
